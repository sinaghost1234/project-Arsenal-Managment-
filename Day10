}
void Adminreportsection (void)
{
    Reports *temp2 = head2 ;

    Products *temp = head ;

    Members *temp1 = head1 ;

    system("cls") ;

    printf("SINA'S ARSENAL MANAGEMENT\n\n") ;

    printf("Report sections\n\n") ;

    printf("Enter the section:\n\n") ;

    printf("****AMOUNTS OF PRODUCT****\n") ;

    printf("1.Number of products in the Arsenal\n\n") ;

    printf("2.Number of a specific Brand of Products in the Arsenal\n\n") ;

    printf("3.Number of a specific model of a Brand product in the Arsenal\n\n") ;

    printf("4.The list of products of which we have more than a certain amount ( you need to enter )\n\n") ;

    printf("5.The list of products of which we have less than a certain amount ( you need to enter )\n\n") ;

    printf("6.The number of arrivals of a specific product ( you need to enter )in a specific historical period ( you need to enter )\n\n") ;

    printf("7.The number of arrivals of all products in a specific time\n\n") ;

    printf("8.The number of exits of a specific product ( you need to enter )in a specific historical period ( you need to enter )\n\n") ;

    printf("9.The number of exits of all products in a specific historical period ( you need to enter ) \n\n") ;

    printf("****DOLLAR VALUES****\n") ;

    printf("10.The dollar value of all products in Arsenal\n\n") ;

    printf("11.The dollar value of a specific Brand in Arsenal\n\n") ;

    printf("12.The dollar value of a specific Model of a Brand in Arsenal\n\n") ;

    printf("****TRANSACTION****\n") ;

    printf("13.The certain amount ( you need to enter ) of last transaction of type of goods (you need to enter) \n\n") ;

    printf("14.The list of transactions registered by a specific user \n\n") ;

    printf("15.The list of transactions registered by a specific user in a specific historical period (you need to enter )") ;

    printf("16.The list of last certain amount (you need to enter) of transactions a specific user (you need to search)\n\n") ;

    printf("17.Back to first menu\n\n") ;

    printf(">>>") ;

    int enter_reportadminchoice ;

    scanf("%d" , &enter_reportadminchoice) ;

    if ( enter_reportadminchoice == 1  )
        {
            system("cls") ;

            printf("SINA'S ARSENAL MANAGEMENT\n\n") ;

            printf("Number of products in the Arsenal\n\n") ;

            amount = 0 ;

            while ( temp != NULL)
            {
                    amount = amount + temp->mounts_product ;

                    temp = temp->next ;
            }

            printf("\nThe Amount of ALL PRODUCTS in the  Arsenal is %d\n" , amount) ;

            printf("\nPress any key to continue ...") ;

            getchar() ;

            Adminreportsection() ;

        }
        else if ( enter_reportadminchoice == 2 )
        {
            system("cls") ;

            printf("SINA'S ARSENAL MANAGEMENT\n\n") ;

            printf("Number of a specific Brand of Products in the Arsenal\n\n") ;

            amount = 0 ;

            printf("Enter the specific Brand in your mind\n") ;

            printf(">>>") ;

            gets(Brand_productreportsearch) ;

            strlwr(Brand_productreportsearch) ;

            while ( temp != NULL )
            {
                if (strcmp (Brand_productreportsearch , temp->Brand)== 0 )
                {

                    amount = temp->mounts_product * temp->price_product ;

                    printf("Brand Type : %s \nProducts Type : %s \nModel Type : %s \nPrice : %d \nAmount : %d \nTotal Price : %d\n" , temp->Brand , temp->name_product,

                           temp->Model_product , temp->price_product , temp->mounts_product , amount ) ;

                    printf("----------------------------") ;

                 }
                temp = temp->next ;

            }
            if ( temp == NULL )
            {
                printf("\n\nTHERE IS NO PRODUCT IN THE DATA BASE WITH YOUR BRAND ENTER\n") ;

                printf("\nPress any key to continue ...") ;

                getchar() ;

                Adminreportsection() ;

            }
            printf("\nPress any key to continue ...") ;

            getchar() ;

            Adminreportsection() ;

        }
        else if ( enter_reportadminchoice == 3)
        {
            system("cls") ;

            printf("SINA'S ARSENAL MANAGEMENT\n\n") ;

            printf("Number of a specific model of a Brand product in the Arsenal\n\n") ;

            printf("Enter the specific Brand in your mind\n") ;

            printf(">>>") ;

