            for ( b = 0 ; b <= 11 ; b++ )
            {
                printf("%d.%d |" ,i , b+1 ) ;

                counter_2 = arr[b] / 100 ;

                counter_2 = counter_2 * (-1) ;

                for ( d = 0 ; d <= counter_2 ; d++ )
                {
                    printf("%c" , (char)254u) ;

                    printf(" ") ;
                }
                printf("\n") ;
            }
            printf("\nPress Any key to continue...") ;

            getchar() ;

            Adminreportsection() ;


        }
        else if ( enter_reportadminchoice == 31)
        {
        system("cls") ;

        printf("SINA'S ARSENAL MANAGEMENT\n\n") ;

        printf("Back to first page...") ;

        Sleep(3000) ;

        AdminMenu() ;

        }
        else if ( enter_reportadminchoice > 31 || enter_reportadminchoice < 1 )
        {
            printf("Wrong enter choice") ;

            Sleep(3000) ;

            Adminreportsection() ;
        }
}
