void checkpassword ( char str5 [] )
{
        int p = 0 ;

        while ( str5[p - 1 ]!= '\r' )
            {
                    str5[p] = getch() ;

                if (str5[p - 1 ]!= '\r')
                {
                    printf("*") ;
                }

                p++ ;

            }
            str5[p-1] = '\0' ;
}
